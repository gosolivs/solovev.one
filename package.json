{
  "name": "solovev.one",
  "description": "My portfolio",
  "scripts": {
    "start": "parcel serve src/pages/index.pug",
    "build": "NODE_ENV=production node ./build.js"
  },
  "dependencies": {
    "intersection-observer": "^0.5.1",
    "riot-route": "^3.1.4"
  },
  "devDependencies": {
    "@gfx/zopfli": "^1.0.11",
    "autoprefixer": "^9.4.7",
    "chalk": "^2.4.2",
    "css-mqpacker": "^7.0.0",
    "iltorb": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "p-queue": "^4.0.0",
    "parcel-bundler": "^1.11.0",
    "postcss": "^7.0.14",
    "postcss-custom-properties": "^8.0.9",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-pxtorem": "^4.0.1",
    "posthtml": "^0.11.3",
    "pug": "^2.0.3",
    "recursive-readdir": "^2.2.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "browserslist": [
    "last 4 versions",
    "not dead",
    "ie >= 11"
  ],
  "postcss": {
    "plugins": {
      "postcss-pxtorem": {
        "rootValue": 16,
        "mediaQuery": true,
        "propList": [
          "font",
          "font-size",
          "line-height",
          "letter-spacing",
          "margin",
          "padding"
        ]
      },
      "postcss-flexbugs-fixes": {},
      "autoprefixer": {}
    }
  }
}
