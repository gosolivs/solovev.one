{
  "name": "solovev.one",
  "description": "My portfolio",
  "version": "1.0.1",
  "scripts": {
    "start": "parcel serve src/pages/index.pug",
    "build": "NODE_ENV=production node ./build.js"
  },
  "dependencies": {
    "intersection-observer": "^0.5.1",
    "riot-route": "^3.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "css-mqpacker": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-compress": "^1.0.7",
    "postcss": "^7.0.14",
    "postcss-custom-properties": "^8.0.9",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-pxtorem": "^4.0.1",
    "posthtml": "^0.11.3",
    "pug": "^2.0.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "browserslist": [
    "last 4 versions",
    "not dead",
    "ie >= 11"
  ],
  "postcss": {
    "plugins": {
      "postcss-pxtorem": {
        "rootValue": 16,
        "mediaQuery": true,
        "propList": [
          "font",
          "font-size",
          "line-height",
          "letter-spacing",
          "margin",
          "padding"
        ]
      },
      "postcss-flexbugs-fixes": {},
      "autoprefixer": {}
    }
  },
	"compress": {
		"test": ".",
		"gzip": {
			"enabled": true,
			"numiterations": 15,
			"blocksplitting": true,
			"blocksplittinglast": false,
			"blocksplittingmax": 15
		},
		"brotli": {
			"enabled": true,
			"mode": 0,
			"quality": 11,
			"lgwin": 22,
			"lgblock": 0,
			"enable_dictionary": true,
			"enable_transforms": false,
			"greedy_block_split": false,
			"enable_context_modeling": false
		}
	}
}
