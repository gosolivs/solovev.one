{
	"extends": "tslint:recommended",
	"rules": {
		"object-literal-sort-keys": false,
		"interface-name": false,
		"max-line-length": {
			"options": [120]
		},
		"no-console": {
			"severity": "warning",
			"options": ["debug", "info", "log", "time", "timeEnd", "trace"]
		},
		"ordered-imports": false,
		"indent": {
			"options": ["tabs"]
		}
	}
}
